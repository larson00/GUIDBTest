<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchor" prefHeight="246.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Button fx:id="viewTeam" layoutX="35.0" layoutY="20.0" mnemonicParsing="false" onAction="#viewTeam" prefHeight="25.0" prefWidth="108.0" text="View Team" />
      <ComboBox fx:id="comboBox" layoutX="521.0" layoutY="20.0" onAction="#selectTeam" prefWidth="150.0" promptText="Select Team" />
      <TableView fx:id="teamTable" layoutX="219.0" layoutY="90.0" prefHeight="150.0" prefWidth="452.0">
        <columns>
          <TableColumn fx:id="teamColumn" prefWidth="75.0" text="Team" />
          <TableColumn fx:id="divisionColumn" prefWidth="75.0" text="Division" />
            <TableColumn fx:id="overallColumn" prefWidth="75.0" text="Overall" />
            <TableColumn fx:id="offenseColumn" prefWidth="75.0" text="Offense" />
            <TableColumn fx:id="defenseColumn" prefWidth="75.0" text="Defense" />
            <TableColumn fx:id="starterColumn" prefWidth="75.0" text="Starter" />
        </columns>
      </TableView>
      <Button fx:id="updateRank" layoutX="35.0" layoutY="111.0" mnemonicParsing="false" onAction="#updateRank" prefHeight="25.0" prefWidth="108.0" text="Update Rank" />
   </children>
</AnchorPane>
